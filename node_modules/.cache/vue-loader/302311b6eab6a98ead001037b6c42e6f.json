version":3,"sources":["SidebarLogo.vue"],"names":[],"mappings":";;;;;;;;AAQA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"SidebarLogo.vue","sourceRoot":"src/layouts/sidebar","sourcesContent":["<template>\n    <div class=\"layout-sidebar-logo\">\n        <img src=\"../../assets/logo.svg\" style=\"height: 32px;\" alt=\".logo\">\n        <h1 v-show=\"!localCollapsed\" :style=\"titleStyles\">{{system.title}}</h1>\n    </div>\n</template>\n\n<script>\n    import {mapState} from 'vuex';\n\n    export default {\n        name: \"SidebarLogo\",\n        props: {\n            collapsed: {\n                type: Boolean,\n                default: false\n            }\n        },\n        data() {\n            return {\n                localCollapsed: this.collapsed\n            }\n        },\n        computed: {\n            ...mapState({\n                theme: state => state.setting.theme,\n                system: state => state.setting.system\n            }),\n            titleStyles() {\n                const themeStyle = this.theme.style;\n                const themeColor = this.theme.color;\n\n                return {\n                    fontSize: '22px',\n                    margin: '0 0 0 12px',\n                    color: themeStyle === 'light' ? themeColor : 'white',\n                }\n            }\n        },\n        watch: {\n            collapsed(value) {\n                if (!value) setTimeout(() => this.localCollapsed = value, 100);\n                else this.localCollapsed = value;\n            }\n        }\n    }\n</script>\n"]}]}