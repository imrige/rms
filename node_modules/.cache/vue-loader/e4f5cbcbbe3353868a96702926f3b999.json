 <a-list-item>\n                                <a-list-item-meta>\n                                    <Greeting slot=\"title\" :username=\"user.userNick\"/>\n                                    <template slot=\"description\">\n                                        <p class=\"margin-bottom-xs\">{{`${user.userDepartment || '暂无部门'} |\n                                            上次登录：${user.userRecentTime}`}}</p>\n                                        <a-tag color=\"blue\">{{user.roleName}}</a-tag>\n                                    </template>\n                                    <a-avatar slot=\"avatar\" :size=\"64\" :src=\"user.userAvatar\"/>\n                                </a-list-item-meta>\n                            </a-list-item>\n                        </a-list>\n                    </a-col>\n                    <a-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"8\" :xl=\"8\">\n                        <Information align=\"center\">\n                            <InformationItem title=\"待办任务\" content=\"3\"/>\n                            <InformationItem title=\"今日来访\" content=\"8\"/>\n                            <InformationItem title=\"总访问量\" content=\"325\"/>\n                        </Information>\n                    </a-col>\n                </a-row>\n            </a-card>\n        </a-col>\n\n        <a-col :span=\"24\">\n            <a-row :gutter=\"24\">\n                <a-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"16\" :xl=\"16\">\n                    <a-card title=\"进行中的项目\" :bordered=\"false\" :loading=\"loading\" class=\"margin-bottom-lg\">\n                        <router-link slot=\"extra\" to=\"/\">全部项目</router-link>\n                        <a-card-grid style=\"width: 33.33%; text-align: center\">Content</a-card-grid>\n                        <a-card-grid style=\"width: 33.33%; text-align: center\">Content</a-card-grid>\n                        <a-card-grid style=\"width: 33.33%; text-align: center\">Content</a-card-grid>\n                        <a-card-grid style=\"width: 33.33%; text-align: center\">Content</a-card-grid>\n                        <a-card-grid style=\"width: 33.33%; text-align: center\">Content</a-card-grid>\n                        <a-card-grid style=\"width: 33.33%; text-align: center\">Content</a-card-grid>\n                    </a-card>\n\n                    <a-card title=\"动态\" :bordered=\"false\" :loading=\"loading\" class=\"margin-bottom-lg\">\n                        <router-link slot=\"extra\" to=\"/\">更多日志</router-link>\n\n                        <a-list itemLayout=\"horizontal\" :data-source=\"actionLogs\">\n                            <a-list-item slot=\"renderItem\" slot-scope=\"item\">\n                                <a-list-item-meta>\n                                    <template slot=\"title\">\n                                        你于\n                                        <strong>{{item.createTime}}</strong>\n                                        在IP地址为\n                                        <span>{{item.ip}}</span>\n                                        的主机上进行了\n                                        <a>{{item.name}}</a>\n                                        操作, 耗时\n                                        <strong>{{item.actionTime}}</strong>\n                                        毫秒\n                                    </template>\n                                    <template slot=\"description\">\n                                        {{`执行方法：${item.method}`}}\n                                    </template>\n                                    <a-avatar slot=\"avatar\" src=\"/static/avatar/ali_dog_img.png\"/>\n                                </a-list-item-meta>\n                                <template slot=\"actions\"></template>\n                            </a-list-item>\n                        </a-list>\n                    </a-card>\n                </a-col>\n\n                <a-col :xs=\"24\" :sm=\"24\" :md=\"24\" :lg=\"8\" :xl=\"8\">\n                    <a-card title=\"快速开始/便捷导航\" :bordered=\"false\" :loading=\"loading\" class=\"margin-bottom-lg\">\n                        <a-row :gutter=\"gutter\">\n                            <a-col :span=\"6\"><a\n                                    style=\"display: inline-block; margin-bottom: 16px; color: rgba(0,0,0,.87)\">操作一</a>\n                            </a-col>\n                            <a-col :span=\"6\"><a\n                                    style=\"display: inline-block; margin-bottom: 16px; color: rgba(0,0,0,.87)\">操作二</a>\n                            </a-col>\n                            <a-col :span=\"6\"><a\n                                    style=\"display: inline-block; margin-bottom: 16px; color: rgba(0,0,0,.87)\">操作三</a>\n                            </a-col>\n                            <a-col :span=\"6\"><a\n                                    style=\"display: inline-block; margin-bottom: 16px; color: rgba(0,0,0,.87)\">操作四</a>\n                            </a-col>\n                            <a-col :span=\"6\"><a\n                                    style=\"display: inline-block; margin-bottom: 16px; color: rgba(0,0,0,.87)\">操作五</a>\n                            </a-col>\n                            <a-col :span=\"6\"><a\n                                    style=\"display: inline-block; margin-bottom: 16px; color: rgba(0,0,0,.87)\">操作六</a>\n                            </a-col>\n                            <a-col :span=\"6\">\n                                <a-button size=\"small\" icon=\"plus\" type=\"primary\" ghost>添加</a-button>\n                            </a-col>\n                        </a-row>\n                    </a-card>\n\n                    <a-card title=\"来访统计\" :bordered=\"false\" :loading=\"loading\" class=\"margin-bottom-lg\">\n                        <Chart type=\"bar\" :dataSource=\"viewsDataSource\" x=\"title\" y=\"value\" name=\"name\" group/>\n                    </a-card>\n                </a-col>\n            </a-row>\n        </a-col>\n    </a-row>\n</template>\n\n<script>\n    import {Chart, Greeting, Information, InformationItem} from \"@/components\";\n    import {WorkbenchApi} from '@/api';\n    import {mapState} from 'vuex';\n\n    let viewsDataSource = [\n        {name: \"我\", title: \"周一\", value: 0},\n        {name: \"总量\", title: \"周一\", value: 53},\n        {name: \"我\", title: \"周二\", value: 4},\n        {name: \"总量\", title: \"周二\", value: 48},\n        {name: \"我\", title: \"周三\", value: 6},\n        {name: \"总量\", title: \"周三\", value: 69},\n        {name: \"我\", title: \"周四\", value: 10},\n        {name: \"总量\", title: \"周四\", value: 53},\n        {name: \"我\", title: \"周五\", value: 7},\n        {name: \"总量\", title: \"周五\", value: 56},\n        {name: \"我\", title: \"周六\", value: 2},\n        {name: \"总量\", title: \"周六\", value: 10},\n        {name: \"我\", title: \"周日\", value: 1},\n        {name: \"总量\", title: \"周日\", value: 8},\n    ];\n    let actionLogs = [],\n        loginLogs = [];\n\n    export default {\n        name: \"Home\",\n        inject: ['gutter'],\n        components: {Chart, Information, InformationItem, Greeting},\n        data() {\n            return {\n                viewsDataSource,\n                actionLogs,\n                loginLogs,\n                loading: false,\n            }\n        },\n        computed: {\n            ...mapState({\n                user: state => state.account.user\n            })\n        },\n        mounted() {\n            this.getIndexInfo();\n        },\n        methods: {\n            getIndexInfo() {\n                WorkbenchApi.getInfos(this.user.userName, {size: 5}).then(res => {\n                    this.actionLogs = res.actionLogs;\n                    this.loginLogs = res.loginLogs;\n                })\n            },\n        }\n    }\n</script>\n"]}]}