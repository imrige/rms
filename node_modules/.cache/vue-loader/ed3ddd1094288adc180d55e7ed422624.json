 Pagination from \"@/common/domain/Pagination\";\n    import {mapState} from 'vuex';\n\n    let columns = [\n        {\n            dataIndex: 'name',\n            key: 'name',\n            title: '名称',\n            width: 100\n        },\n        {\n            dataIndex: 'method',\n            key: 'method',\n            scopedSlots: {customRender: 'method'},\n            title: '方法'\n        },\n        {\n            dataIndex: 'actionTime',\n            key: 'actionTime',\n            title: '耗时',\n            align: 'right',\n            scopedSlots: {customRender: 'actionTime'},\n            width: 100\n        },\n        {\n            dataIndex: 'createTime',\n            key: 'createTime',\n            title: '时间',\n            align: 'right',\n            width: 200\n        },\n    ];\n\n    export default {\n        name: \"Log\",\n        inject: ['gutter'],\n        components: {Tips},\n        data() {\n            return {\n                columns,\n                actionLogs: {}\n            }\n        },\n        computed: {\n            ...mapState({\n                user: state => state.account.user\n            })\n        },\n        mounted() {\n            this.getActionLogs();\n        },\n        methods: {\n            getActionLogs() {\n                ActionLogApi.get(this.user.userName, new Pagination()).then(res => {\n                    this.actionLogs = res;\n                    this.actionLogs.records = this.actionLogs.records.map(item => {\n                        item.key = item.id;\n                        return item;\n                    });\n                });\n            }\n        }\n    }\n</script>\n"]}]}