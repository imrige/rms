=\"small\"\n                              v-model=\"theme.fixHeader\"/>\n                </a-list-item>\n                <a-list-item>\n                    <a-list-item-meta description=\"固定侧边栏\"/>\n                    <a-switch slot=\"actions\" size=\"small\"\n                              v-model=\"theme.fixSidebar\" :disabled=\"theme.layout !== 'side'\"/>\n                </a-list-item>\n                <a-list-item>\n                    <a-list-item-meta description=\"默认折叠侧边栏\"/>\n                    <a-switch slot=\"actions\" size=\"small\"\n                              v-model=\"theme.collapseSidebar\"/>\n                </a-list-item>\n            </a-list>\n            <a-divider/>\n\n            <a-button block @click=\"setTheme($_.cloneDeep(defaultTheme))\" icon=\"reload\">恢复默认设置</a-button>\n        </a-drawer>\n    </p>\n</template>\n\n<script>\n    import {mapState} from 'vuex';\n\n    export default {\n        name: \"HeaderSetting\",\n        data() {\n            return {\n                visible: false\n            }\n        },\n        computed: {\n            ...mapState({\n                theme: state => state.setting.theme,\n                defaultTheme: state => state.setting.defaultTheme,\n            })\n        },\n        methods: {\n            setTheme(theme) {\n                console.log(theme);\n            }\n        },\n        watch: {\n            theme: {\n                handler(value) {\n                    this.setTheme(value);\n                },\n                deep: true\n            }\n        }\n    }\n</script>\n"],"sourceRoot":"src/layouts/header"}]}