        </RadioGroup>\n            <a-divider/>\n\n            <h3>导航设置</h3>\n            <a-list :split=\"false\" size=\"small\">\n                <a-list-item>\n                    <a-list-item-meta description=\"固定导航栏\"/>\n                    <a-switch slot=\"actions\" size=\"small\" v-model=\"theme.themeFixedHeader\"/>\n                </a-list-item>\n                <a-list-item>\n                    <a-list-item-meta description=\"固定侧边栏\"/>\n                    <a-switch slot=\"actions\" size=\"small\" v-model=\"theme.themeFixedSidebar\"/>\n                </a-list-item>\n                <a-list-item>\n                    <a-list-item-meta description=\"默认折叠侧边栏\"/>\n                    <a-switch slot=\"actions\" size=\"small\" v-model=\"theme.themeCollapsedSidebar\"/>\n                </a-list-item>\n            </a-list>\n            <a-divider/>\n\n            <h3>布局设置</h3>\n            <RadioGroup v-model=\"theme.themeLayout\">\n                <Radio title=\"侧边菜单布局\" value=\"side\" style=\"margin-right: 16px;\"\n                       src=\"https://gw.alipayobjects.com/zos/antfincdn/XwFOFbLkSM/LCkqqYNmvBEbokSDscrm.svg\"/>\n                <Radio title=\"顶部菜单布局\" value=\"top\" style=\"margin-right: 16px;\"\n                       src=\"https://gw.alipayobjects.com/zos/antfincdn/URETY8%24STp/KDNDBbriJhLwuqMoxcAr.svg\"/>\n            </RadioGroup>\n            <a-divider/>\n\n            <a-button block @click=\"setTheme($_.cloneDeep(defaultTheme))\" icon=\"reload\">恢复默认设置</a-button>\n        </a-drawer>\n    </p>\n</template>\n\n<script>\n    import {ThemeApi} from '@/api';\n    import {RadioGroup, Radio} from '@/components';\n    import {mapState} from 'vuex';\n\n    export default {\n        name: \"HeaderSetting\",\n        components: {Radio, RadioGroup},\n        data() {\n            return {\n                visible: false\n            }\n        },\n        computed: {\n            ...mapState({\n                theme: state => state.setting.theme,\n                defaultTheme: state => state.setting.defaultTheme,\n                user: state => state.account.user\n            })\n        },\n        methods: {\n            setTheme(theme) {\n                const loading = this.$message.loading(\"更新主题中...\", 0);\n                theme.themeId = this.user.userId;\n                ThemeApi.modify(theme).then(() => setTimeout(loading, 300));\n            }\n        },\n        watch: {\n            theme: {\n                handler(value) {\n                    this.setTheme(value);\n                },\n                deep: true\n            }\n        }\n    }\n</script>\n"],"sourceRoot":"src/views/layout/header"}]}