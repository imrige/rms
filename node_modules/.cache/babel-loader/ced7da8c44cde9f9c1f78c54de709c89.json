          <a-list itemLayout=\"horizontal\" :dataSource=\"actionLogs.records\" size=\"small\">\n                    <a-list-item slot=\"renderItem\" slot-scope=\"item, index\">\n                        <a-list-item-meta>\n                            <template slot=\"title\">\n                                <p>{{item.name}}</p>\n                            </template>\n                            <template slot=\"description\">\n                                <p class=\"text-ellipsis\" style=\"width: 500px\" :title=\"item.method\">\n                                    方法：{{item.method}}</p>\n                                <p class=\"text-ellipsis\" style=\"width: 500px\" :title=\"item.params\">参数：{{item.params ||\n                                    '-'}}</p>\n                            </template>\n                        </a-list-item-meta>\n\n                        <Information :split=\"false\" size=\"small\">\n                            <InformationItem title=\"位置\" :content=\"item.location || '未知'\"/>\n                            <InformationItem title=\"耗时\" :content=\"`${item.actionTime}ms`\"/>\n                            <InformationItem title=\"IP\" :content=\"item.ip\"/>\n                        </Information>\n\n                        <template slot=\"actions\">\n                            {{item.createTime}}\n                        </template>\n                    </a-list-item>\n                </a-list>\n            </a-col>\n        </a-row>\n    </a-drawer>\n</template>\n\n<script>\n    import {ActionLogApi} from '@/api';\n    import {Information, InformationItem, Profile, Tips} from \"@/components\";\n    import Pagination from \"@/common/domain/Pagination\";\n\n    let actionLogs = {};\n\n    export default {\n        name: \"UserDetail\",\n        components: {Information, InformationItem, Profile, Tips},\n        props: {\n            value: {\n                type: Boolean,\n                default: false\n            },\n            user: {\n                type: Object,\n                default() {\n                    return {}\n                }\n            }\n        },\n        data() {\n            return {\n                actionLogs\n            }\n        },\n        methods: {\n            getActionLogs() {\n                ActionLogApi.get(this.user.userName, new Pagination(1, 5)).then(res => this.actionLogs = res);\n            },\n            bindClickClose() {\n                this.$emit('input', false);\n            },\n        },\n        watch: {\n            value(value) {\n                if (value) this.getActionLogs();\n            }\n        }\n    }\n</script>\n"],"sourceRoot":"src/views/page/system/user"}]}