-item label=\"昵称\">\n                    <a-input placeholder=\"请输入\"\n                             v-decorator=\"['userNick', {initialValue: user.userNick, rules: [{ required: true, message: '昵称不能为空!' }]}]\"/>\n                </a-form-item>\n                <a-form-item label=\"性别\">\n                    <a-radio-group :options=\"options\"\n                                   v-decorator=\"['userSex', {initialValue: user.userSex, rules: [{ required: true, message: '请选择性别!' }]}]\"/>\n                </a-form-item>\n                <a-form-item label=\"个人简介\">\n                    <a-textarea placeholder=\"请输入\" :rows=\"4\"\n                                v-decorator=\"['userDescription', {initialValue: user.userDescription}]\"/>\n                </a-form-item>\n                <a-form-item>\n                    <a-button type=\"primary\" html-type=\"submit\" block :loading=\"loading\">更新信息</a-button>\n                </a-form-item>\n            </a-form>\n        </a-col>\n        <a-col :span=\"12\">\n            <a-form>\n                <a-form-item label=\"头像\">\n                    <a-tooltip title=\"点击选择\" placement=\"bottom\">\n                        <a-avatar :src=\"user.userAvatar\" :size=\"144\" class=\"cursor-pointer\"/>\n                    </a-tooltip>\n                </a-form-item>\n            </a-form>\n        </a-col>\n    </a-row>\n</template>\n\n<script>\n    import {mapState, mapMutations} from 'vuex';\n\n    let options = [\n        {label: '男', value: 1},\n        {label: '女', value: 0},\n        {label: '保密', value: -1},\n    ];\n\n    export default {\n        name: \"MenuBasic\",\n        data() {\n            return {\n                options,\n                form: this.$form.createForm(this),\n                loading: false\n            }\n        },\n        computed: {\n            ...mapState({\n                user: state => state.account.user\n            })\n        },\n        methods: {\n            ...mapMutations({\n                setUser: 'account/setUser'\n            }),\n            apiUpdateUser(user) {\n                this.loading = true;\n                return this.$request.put('/user', user)\n                    .then(res => {\n                        this.loading = false;\n                        if (res.data.code === 0) return res;\n                    })\n                    .catch(() => this.loading = false);\n            },\n            bindClickSubmit(e) {\n                e.preventDefault();\n                this.form.validateFields((err, values) => {\n                    if (!err) {\n                        values = Object.assign(this.user, values);\n\n                        delete values.userPass;\n\n                        this.apiUpdateUser(values).then(() => {\n                            this.$message.success(\"更新成功\");\n                            this.setUser(values);\n                        });\n                    }\n                });\n            }\n        }\n    }\n</script>\n"],"sourceRoot":"src/views/page/account/setting/menu"}]}